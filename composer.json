        "phing/phing": "2.*",
        "etsy/phan": "0.8.0",
        "phpmd/phpmd": "^2.5",
        "squizlabs/php_codesniffer": "2.*",
        "sebastian/phpcpd": "*",
        "magento-ecg/coding-standard": "*",
